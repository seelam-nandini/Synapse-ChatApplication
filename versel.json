{
    "version": 2,
    "projects": {
      "client": {
        "src": "client/",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "build"
        }
      },
      "server": {
        "src": "server/",
        "use": "@vercel/node"
      },
      "socket": {
        "src": "socket/",
        "use": "@vercel/node"
      }
    },
    "routes": [
      {
        "src": "/socket/(.*)",
        "dest": "/socket/index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  
  